{
  "version": 3,
  "framework": null,
  "outputDirectory": ".",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ]
}
